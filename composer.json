{
    "name": "ray/aop",
    "description": "An aspect oriented framework",
    "keywords": [
        "aop",
        "AOP Alliance",
        "code-gen",
        "annotations"
    ],
    "homepage": "https://github.com/koriym/Ray.Aop",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "doctrine/annotations": "~1.1",
        "nikic/php-parser": "~1.0"
    },
    "autoload": {
        "psr-4": { "Ray\\Aop\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ray\\Aop\\": "tests/" },
        "psr-4": { "Ray\\Aop\\": "tests/Fake/" },
        "files": ["tests/Fake/FakeAnnotateClassNoName.php"]
    },
    "suggest": {
        "ray/di": "A dependency injection framework with Ray.Aop"
    },
    "extra": {
        "branch-alias": {
            "dev-develop-2": "2.0.x-dev"
        }
    }
}
